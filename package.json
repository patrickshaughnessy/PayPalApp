{
  "name": "PayPalApp",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": "6.1.0"
  },
  "scripts": {
    "postinstall": "npm run build:dll",
    "heroku-prebuild": "npm install --dev",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:clean": "npm run test:clean && rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "npm run build && node app.js",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "start:dev": "source .env/development && DEBUG=PayPalApp:* node app.js",
    "start:prod": "npm run build && source .env/production && DEBUG=PayPalApp:* node app.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "standard --verbose | snazzy",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "test:server": "source .env/test && DEBUG=PayPalApp:* mocha --recursive -R spec",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "standard": {
    "ignore": [
      "internals/scripts"
    ],
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "before",
      "fetch",
      "Response"
    ]
  },
  "lint-staged": {
    "*.js": "lint:js",
    "*.css": "lint:css"
  },
  "pre-commit": {
    "silent": false,
    "run": [
      "lint"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": 2
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "apisauce": "^0.4.0",
    "async": "^2.0.1",
    "babel-polyfill": "6.13.0",
    "body-parser": "~1.13.2",
    "cldr-core": "^29.0.0",
    "cldr-misc-full": "^29.0.0",
    "compression": "1.6.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "4.14.0",
    "fontfaceobserver": "2.0.1",
    "history": "3.0.0",
    "immutable": "3.8.1",
    "intl": "1.2.4",
    "invariant": "^2.2.1",
    "ip": "1.1.3",
    "jade": "~1.11.0",
    "lodash": "4.14.1",
    "lodash.merge": "^4.5.1",
    "material-ui": "^0.15.4",
    "mongoose": "^4.5.8",
    "mongoose-deep-populate": "^3.0.0",
    "morgan": "~1.6.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-flexr": "^2.1.0-beta",
    "react-helmet": "3.1.0",
    "react-intl": "^2.1.3",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "4.0.5",
    "react-router-scroll": "0.2.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "3.5.2",
    "redux-immutable": "3.0.6",
    "redux-logger": "^2.6.1",
    "redux-saga": "0.11.0",
    "redux-seamless-immutable": "^0.1.5",
    "reduxsauce": "^0.1.0",
    "request": "^2.74.0",
    "reselect": "2.5.3",
    "rootpath": "^0.1.2",
    "sanitize.css": "4.1.0",
    "seamless-immutable": "^6.1.1",
    "warning": "^3.0.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "cheerio": "0.20.0",
    "colors": "^1.1.2",
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-import-resolver-webpack": "0.4.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "6.0.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.1.2",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-intl-shim": "^1.0.3",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "lint-staged": "2.0.3",
    "minimist": "1.2.0",
    "mocha": "3.0.1",
    "muicss": "^0.7.0",
    "ngrok": "2.2.2",
    "null-loader": "0.1.1",
    "offline-plugin": "3.4.2",
    "plop": "1.5.0",
    "postcss-cssnext": "2.7.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "0.9.1",
    "postcss-reporter": "1.4.1",
    "pre-commit": "1.1.3",
    "psi": "2.0.4",
    "rimraf": "2.5.4",
    "shelljs": "0.7.3",
    "sinon": "2.0.0-pre",
    "snazzy": "^4.0.1",
    "standard": "^7.1.2",
    "style-loader": "0.13.1",
    "stylelint": "7.1.0",
    "stylelint-config-standard": "12.0.0",
    "supertest": "^2.0.0",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  }
}
